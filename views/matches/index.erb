<main class="main">
  <h1>Matches</h1>

  <% @matches.each do |match| %>
  <p><%= match.home_team %> <%= match.home_score %> -  <%= match.away_score %> <%= match.away_team %></p>
  <button type="button" name="button"><a class="button-a" href="/matches/<%= match.id %>">View Match</a></button>
  <% end %>

</main>
